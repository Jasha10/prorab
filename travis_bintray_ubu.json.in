{
	"package": {
		"name": "prorab",
		"repo": "deb",
		"subject": "igagis"
	},
	
	"version": {
		"name": "$(version)"
	},

	"files": [
		{
			"includePattern": "../(.[^/]*(\\.deb)$)",
			"uploadPattern": "$1",
			"matrixParams": {
				"deb_distribution": "ubuntu",
				"deb_component": "main",
				"deb_architecture": "amd64"
			}
		}
	],
	"publish": true,
	"override": true
}
